{
    "manifest_version": 2,
    "name": "scoks_toggly",
    "version": "1.0",
    "description": "Toggles the socks proxy setting to on or default (off)",
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistance": false
    },
    "icons": {
        "48": "icons/s_tog_2.png",
        "96": "icons/s_tog_2.png"
    },
    "permissions": [
        "storage",
        "proxy",
        "activeTab"
    ],
    "browser_action": {
        "default_icon": "icons/s_tog_1.png",
        "default_title": "Toggle SOCKS",
        "default_popup": "popup/toggle_socks.html"
    }
}